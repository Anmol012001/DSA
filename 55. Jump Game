class Solution {
public:
    bool canJump(vector<int>& nums) {
        int ind=nums.size()-1;
        int n=nums.size();

        for(int i=n-1;i>=0;i--)
        {
            if(nums[i]+i>=ind)
            {
                ind=i;
            }
        }
        if(ind==0)
        {
            return true;
        }
        return false;
    }
};
